<div class="full">
  <main class="main">
    <div class="types-wrapper">
      <h1>Activity Feed</h1>
      <table>
        <thead>
          <tr>
            <th>Id</th>
            <th>Course</th>
            <th>Teacher</th>
            <th>Title</th>
            <th>Start date</th>
            <th>End date</th>
            <th>Posted date</th>
            <th>Due date</th>
            <th>Graded</th>
            <th>Points</th>
            <th>Instructions</th>
            <th>Responses</th>
            <th colspan="3"></th>
          </tr>
        </thead>
  
        <tbody>
          <% @interactions.each do |interaction| %>
          <tr>
            <td><%= interaction.id %></td>
            <td><%= interaction.course.name %></td>
            <td><%= interaction.user.user_name %></td>
            <td><%= interaction.title %></td>
            <td><%= interaction.start_date %></td>
            <td><%= interaction.end_date %></td>
            <td><%= interaction.posted_date %></td>
            <td><%= interaction.due_date %></td>
            <td><%= interaction.graded %></td>
            <td><%= interaction.points %></td>
            <td><%= interaction.instructions %></td>
            <td><%= interaction.responses.count %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </main>
</div>